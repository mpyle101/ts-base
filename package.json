    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^22.1.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.8.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4"
    }
}
